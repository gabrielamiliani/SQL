Create a clothing store database 

CREATE TABLE gaby_clothing (id INTEGER PRIMARY KEY, name TEXT, quantity INTEGER, season TEXT, price INTEGER);

INSERT INTO gaby_clothing VALUES(1, "Dresses", 5, "Prom", 40);
INSERT INTO gaby_clothing VALUES(2, "Jackets", 3, "Winter", 20);
INSERT INTO gaby_clothing VALUES(3, "Jeans", 2, "Summer", 15);
INSERT INTO gaby_clothing VALUES(4, "Crop Tops", 5,"Spring", 5);

--display the database order by price (highest to lowest)
SELECT * FROM gaby_clothing
ORDER BY price desc;

--what would be the total amount of sales?
SELECT SUM(price) from gaby_clothing

